{
  "name": "Byparr",
  "version": "0.1.0",
  "slug": "byparr",
  "description": "Proxy server to bypass Cloudflare and anti-bot protection using Camoufox browser",
  "url": "https://github.com/ThePhaseless/Byparr",
  "arch": ["amd64"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "ports": {
    "8191/tcp": 8191
  },
  "ports_description": {
    "8191/tcp": "Byparr API endpoint (FlareSolverr compatible)"
  },
  "webui": "http://[HOST]:[PORT:8191]/docs",
  "options": {
    "log_level": "info"
  },
  "schema": {
    "log_level": "list(debug|info|warning|error)?",
    "proxy_server": "str?",
    "proxy_username": "str?",
    "proxy_password": "password?"
  },
  "environment": {
    "HOST": "0.0.0.0",
    "PORT": "8191",
    "PYTHONUNBUFFERED": "1",
    "PYTHONDONTWRITEBYTECODE": "1"
  }
}
